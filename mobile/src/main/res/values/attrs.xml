<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="proportion">
        <attr name="proportion" format="float" />
        <attr name="proportion_to" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="infoView">
        <attr name="info_bg_color" format="color" />
        <attr name="info_text" format="string" />
        <attr name="info_text_color" format="color" />
        <attr name="info_icon" format="reference" />
    </declare-styleable>
</resources>